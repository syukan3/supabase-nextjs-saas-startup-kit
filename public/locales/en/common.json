                "add_users": {
                    "question": "How do I add other users to my account?",
                    "answer": "From the Team Management page, click 'Invite Members' and enter the email addresses of the people you want to invite. They'll receive an invitation email and will be added once they accept."
                },
                "export_data": {
                    "question": "Can I export my data?",
                    "answer": "Yes, you can. Go to the Data Management section in Settings and select 'Export Data'. You can download your data in CSV or JSON format."
                },
